// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  email   String  @unique
  pwd     String
  id      Int     @id @default(autoincrement())
  name    String?
  isAdmin Boolean
}

model Apps {
  id          Int     @id @default(autoincrement())
  title       String
  description String?
  APP_ID      String
  API_KEY     String
}
